{
  "name": "law-body-pol-animation-test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "serve": "vite",
    "build": "tsc && vite build",
    "render:custom": "node render.js --scene=customScene",
    "render:legislative": "node render.js --scene=legislativeGap",
    "render:cost": "node render.js --scene=costBenefit",
    "render:intervention": "node render.js --scene=interventionRates",
    "render:all": "npm run render:custom && npm run render:legislative && npm run render:cost && npm run render:intervention",
    "direct:custom": "vite preview --outDir ./output/customScene -- --scene=customScene",
    "direct:legislative": "vite preview --outDir ./output/legislativeGap -- --scene=legislativeGap",
    "direct:cost": "vite preview --outDir ./output/costBenefit -- --scene=costBenefit",
    "direct:intervention": "vite preview --outDir ./output/interventionRates -- --scene=interventionRates"
  },
  "dependencies": {
    "@motion-canvas/core": "^3.17.2",
    "@motion-canvas/2d": "^3.17.2",
    "@motion-canvas/ffmpeg": "^1.1.0"
  },
  "devDependencies": {
    "@motion-canvas/ui": "^3.17.2",
    "@motion-canvas/vite-plugin": "^3.17.2",
    "typescript": "^5.2.2",
    "vite": "^4.0.0"
  }
}